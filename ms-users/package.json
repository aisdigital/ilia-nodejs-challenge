{
  "name": "ms-users",
  "scripts": {
    "start": "ts-node src/main/server.ts",
    "start:dev": "nodemon src/main/server.ts",
    "scripts": "ts-node",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll",
    "test:all": "jest --passWithNoTests --runInBand",
    "test:coverage": "jest --passWithNoTests --coverage",
    "eslint:fix": "eslint . --fix",
    "orm:create:migration": "typeorm migration:create -n",
    "orm:migrate": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "orm:migrate:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/es6-shim": "^0.31.42",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^4.3.8",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.0",
    "nodemon": "^2.0.14",
    "sucrase": "^3.20.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.38"
  }
}
