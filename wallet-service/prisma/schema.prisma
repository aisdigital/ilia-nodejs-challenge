generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Transaction {
id String @id @default(uuid())
user_id String @map("user_id")
amount Int
type TransactionType
created_at DateTime @default(now())
}


enum TransactionType {
CREDIT
DEBIT
}

